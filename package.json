{
    "name": "meetup-api",
    "version": "1.0.1",
    "main": "index.js",
    "scripts": {
        "clean-deploys": "now rm $INSTANCE_NAME --token $NOW_TOKEN --yes",
        "deploy": "now alias $(now --name $INSTANCE_NAME -e BLACK_LIST=$BLACK_LIST -e CACHE_EXPIRATION=$CACHE_EXPIRATION -e CATEGORIES=$CATEGORIES -e LAT=$LAT -e LON=$LON -e MEETUP_API_KEY=$MEETUP_API_KEY -e RADIUS=$RADIUS --npm --token $NOW_TOKEN) $INSTANCE_NAME --token=$NOW_TOKEN",
        "dev": "micro-dev -p ${PORT:-4002}",
        "precommit": "eslint '**/*.js'",
        "scale": "now scale $INSTANCE_NAME 1 --token $NOW_TOKEN",
        "start": "micro"
    },
    "author": "durancristhian@gmail.com",
    "license": "MIT",
    "devDependencies": {
        "babel-eslint": "^9.0.0",
        "eslint": "^5.4.0",
        "husky": "^0.14.3",
        "micro-dev": "^3.0.0"
    },
    "dependencies": {
        "dotenv": "^6.0.0",
        "got": "^9.1.0",
        "memory-cache": "^0.2.0",
        "micro": "^9.3.2",
        "micro-cors": "^0.1.0",
        "moment": "^2.22.2"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/meetupjs-ar/meetup-api.git"
    }
}
